// 将设置放入此文件中以覆盖默认值和用户设置。
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.html": true,
        "**/*.xlsx": true,
        "**/*.docx": true,
        "**/*.pdf": true
    },
    "python.formatting.provider": "yapf"
}